1.program to swap two numbers using bitwise operator.
#include<stdio.h>
int main(){
int a=10;
int b=20;
a=a^b;
b=a^b;
a=a^b;
printf("%d%d",a,b);
return 0;
}
o/p:a=20,b=10.
2.program to calculate GRADE in your first semester(take input from user).
#include<stdio.h>
#include<conio.h>
int main()
{
int i;
float mark,sum=0,avg;
printf("enter the marks");
for(i=0;i<5;i++)
{
scanf("%f",&mark);
sum=sum+mark;
}
avg=sum/5;
if(avg>=91 && avg<=100)
   printf("A1");
else if(avg>=81 && avg<81)
   printf("A2");
else if(avg>=71 && avg<81)
   printf("B1");
else if(avg>=61 && avg<71)
   printf("B2");
else if(avg>=51 && avg<61)
   printf("C1");
else if(avg>=41 && avg<51)
   printf("C2");
else if(avg>=33 && avg<41)
   printf("D");
else if(avg>=25 && avg<33)
   printf("E1")
else
   printf("FAIL");
return 0;
}
3.program to get minimum number of bits to store an integer number.
#include<stddio.h>
int countBit(int);
int main()
{
int num;
printf("enter the number");
scanf("%d",&num);
printf("%d",countBit(num));
return 0;
}
  int countbit(int n)
{
int count=0,i;
if(n==0)
  return 0;
for(i=0;i<32;i++)
{
  if((1<<i)&n)
     count=i;
 }
 return ++count;
 }
 



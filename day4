1.PROGRAM TO PRINT TABLES FROM NUMBERS 1 TO 20.
 #include<stdio.h>
 int main()
 {
 int i,j;
 int num;
 for(i=1;i<=20;i++)
 {
   num=i;
   for(j=1;j<=20;j++)
   {
      printf("%d",(num*j));
   }
   printf("\n");
 }
 return 0;
 }
2.PROGRAM TO CHECK ENTERED NUMBER IS ZERO,POSITIVE,NEGATIVE UNTIL USER DOES N0T WANT TO QUIT.
  #include<stdio.h>
  int main()
  {
  int num;
  char choice;
  do{
     printf("enter an integer number");
     scanf("%d",&num);
     if(num==0)
     {
         printf("number is zero");
     }
     else if(num>0)
     {
         printf("number is positive");
     }
     else
     {
         printf("number is negative");
     }
     printf("want to chech again (press Y/y for 'yes'): ");
     scanf(" %c",&choice);
     }
     while(choice== 'Y' || chioce=='y');
     {
        printf('bye bye!!!");
     }
     return 0;
    }
  3.PROGRAM TO FIND FACTORIAL OF A NUMBER.
    #include<stdio.h>
    int main()
    {
    int i,fact=1,n;
    printf("enter a number to find factorial");
    scanf("%d",&n);
    for(i=1;;i<=n;i++)
    {
       fact=fact*i;
    }
    printf("Factorial of %d%d",n,fact)
    return 0;
    }
  4.PROGRAM TO FIND SUM OF FIRST NATURAL NUMBERS ,N MUST BE TAKEN BY THE USER.
    #include<stdio.h>
    int main()
    {
    int n,i,sum=0;
    printf(" enter the positive integer");
    scanf("%d",&n);
    for(i=1;i<=n;++i)
    {
       sum+=i;
    }
    printf("sum=%d",sum);
    return 0;
    }
 5.PROGRAM TO PRINT ALL PRIME NUMBERS FROM 1 TO N.
    #include<stdio.h>
    int main()
    {
    int num,i,count,n;
    printf("enter the max range");
    scanf("%d",&n);
    for(num=1;num<=n;num++)
    {
        count=0;
        for(i=2;i<=num/2;i++)
        {
           if(num%i==0)
           {
              count++;
              break;
           }
        }
        if(count==0 && num!=1)
        {
           printf("%d",num);
         }
    }
    return 0;
    }
    
